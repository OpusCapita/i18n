{
  "name": "jcatalog-i18n",
  "version": "8.0.0",
  "description": "Provides i18n utils",
  "main": "index.js",
  "files": [
    "dist",
    "lib",
    "scripts/fix-prepublish.js",
    "src",
    ".babelrc",
    "index.js"
  ],
  "scripts": {
    "clean": "rimraf build dist esdoc lib",
    "lint": "eslint src",
    "jscs": "jscs src",
    "test": "mocha src/**/*.spec.js",
    "build": "npm run clean && npm run lint && npm run jscs && npm test && npm run build-lib && npm run build-umd && npm run build-umd-min",
    "build-lib": "babel src --out-dir lib",
    "build-umd": "cross-env NODE_ENV=development webpack --progress --colors --config webpack.config.js",
    "build-umd-min": "cross-env NODE_ENV=production webpack --progress --colors --config webpack.config.js",
    "grails-plugin-package": "npm run build && npm run esdoc && gulp grails-plugin-package",
    "grails-plugin-install": "npm run grails-plugin-package && gulp grails-plugin-install",
    "grails-plugin-deploy": "npm run grails-plugin-package && gulp grails-plugin-deploy",
    "grails-plugin-release": "npm run grails-plugin-package -- --release && gulp grails-plugin-deploy --release",
    "esdoc": "esdoc -c esdoc.json",
    "postinstall": "node ./scripts/fix-prepublish.js postinstall",
    "prepublish": "npm run build-lib"
  },
  "author": {
    "name": "Dmitry Divin",
    "email": "dmitry.divin@jcatalog.com"
  },
  "license": "UNLICENSED",
  "readme": "README.md",
  "dependencies": {
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "moment": "^2.10.6",
    "moment-jdateformatparser": "git+https://github.com/MadMG/moment-jdateformatparser#88f3580f6f5a281fd678d760e146452bc2f04c97",
    "moment-timezone": "^0.4.0",
    "underscore": "^1.8.3",
    "underscore-deep-extend": "^0.0.5"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.6",
    "babel-loader": "^6.2.1",
    "babel-register": "^6.4.3",
    "chai": "^3.4.1",
    "cross-env": "^1.0.7",
    "css-loader": "^0.21.0",
    "esdoc": "^0.4.3",
    "esdoc-es7-plugin": "0.0.3",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^3.1.0",
    "git-rev-sync": "^1.4.0",
    "globule": "^0.2.0",
    "gulp": "^3.9.0",
    "gulp-util": "^3.0.7",
    "gulp-zip": "^3.1.0",
    "handlebars": "^4.0.5",
    "jscs": "^2.9.0",
    "mocha": "^2.3.4",
    "rimraf": "^2.5.1",
    "semver": "^5.1.0",
    "style-loader": "^0.13.0",
    "through2": "^2.0.0",
    "webpack": "^1.12.2"
  },
  "engines": {
    "node": ">=4.2.4",
    "npm": ">=3.6.0"
  },
  "grails": {
    "version": "2.4.4",
    "javaSrc": "./dist",
    "resources": {
      "i18n-manager-resources": [
        {
          "file": {
            "name": "i18n-bundle.min.js"
          }
        }
      ]
    }
  },
  "maven": {
    "groupId": "com.jcatalog.grailsplugins",
    "artefactId": "jcatalog-i18n-resources",
    "repositories": {
      "releases": {
        "id": "maven2ReleasesDeploymentRepositoryId",
        "url": "http://maven.scand/nexus/content/repositories/releases"
      },
      "snapshots": {
        "id": "maven2SnapshotsDeploymentRepositoryId",
        "url": "http://maven.scand/nexus/content/repositories/snapshots"
      }
    }
  }
}
