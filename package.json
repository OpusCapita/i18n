{
  "name": "jcatalog-i18n",
  "version": "1.1.1",
  "description": "Provides i18n utils",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "clean": "rimraf esdoc",
    "lint": "eslint src",
    "test": "mocha src/**/*.spec.js",
    "esdoc": "esdoc -c esdoc.json",
    "build-npm": "rimraf lib && babel --copy-files --no-babelrc --presets es2015,stage-0 --ignore *.spec.js src --out-dir lib",
    "publish-test": "npm run build-npm && fluid-publish --options=\"{\\\"devTag\\\": \\\"SNAPSHOT\\\"}\" --test",
    "publish-dev": "npm run build-npm && fluid-publish --options=\"{\\\"devTag\\\": \\\"SNAPSHOT\\\"}\"",
    "publish-release": "npm run build-npm && fluid-publish --standard"
  },
  "repository": {
    "type": "git",
    "url": "http://buildserver.jcatalog.com/git/js-i18n.git"
  },
  "author": {
    "name": "Dmitry Divin",
    "email": "dmitry.divin@jcatalog.com"
  },
  "license": "jCatalog",
  "readme": "README.md",
  "dependencies": {
    "moment": "2.10.6",
    "moment-jdateformatparser": "1.0.2",
    "moment-timezone": "0.4.0",
    "underscore": "1.8.3",
    "underscore-deep-extend": "0.0.5"
  },
  "devDependencies": {
    "babel-cli": "6.4.5",
    "babel-core": "6.4.0",
    "babel-eslint": "6.0.4",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-stage-0": "6.3.13",
    "chai": "3.4.1",
    "esdoc": "0.4.3",
    "esdoc-es7-plugin": "0.0.3",
    "eslint": "2.10.2",
    "eslint-config-jcatalog": "1.0.0",
    "eslint-plugin-react": "5.1.1",
    "mocha": "2.4.5",
    "rimraf": "2.5.1",
    "fluid-publish": "1.1.0"
  },
  "engines": {
    "node": "4.x",
    "npm": "2.x"
  }
}
