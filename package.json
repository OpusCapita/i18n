{
  "name": "jcatalog-i18n",
  "version": "1.1.4",
  "description": "Provides i18n utils",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "clean": "rimraf esdoc",
    "lint": "eslint src",
    "test": "rimraf ./.nyc_output ./coverage && cross-env nyc mocha --recursive 'src/**/*.spec.js'",
    "testonly": "mocha src/**/*.spec.js --require config/mocha-setup.js",
    "npm-build": "rimraf lib && babel --copy-files --no-babelrc --presets es2015,stage-0 --ignore *.spec.js src --out-dir lib",
    "npm-publish": "npm run npm-build && npm-publish",
    "publish-snapshot": "npm run npm-publish",
    "publish-release": "npm run npm-publish -- --release"
  },
  "repository": {
    "type": "git",
    "url": "http://buildserver.jcatalog.com/git/js-i18n.git"
  },
  "author": {
    "name": "Dmitry Divin",
    "email": "dmitry.divin@jcatalog.com"
  },
  "license": "jCatalog",
  "readme": "README.md",
  "dependencies": {
    "lodash": "4.16.4",
    "moment": "2.10.6",
    "moment-jdateformatparser": "git+https://github.com/MadMG/moment-jdateformatparser#88f3580f6f5a281fd678d760e146452bc2f04c97",
    "underscore": "1.8.3",
    "underscore-deep-extend": "0.0.5"
  },
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-core": "6.18.2",
    "babel-eslint": "6.0.4",
    "babel-plugin-istanbul": "2.0.3",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-stage-0": "6.16.0",
    "babel-register": "6.18.0",
    "chai": "3.5.0",
    "cross-env": "3.1.3",
    "eslint": "2.10.2",
    "eslint-config-jcatalog": "1.0.2",
    "eslint-plugin-react": "5.1.1",
    "jcatalog-dev-tools": "2.0.14",
    "lodash": "4.16.6",
    "mocha": "3.1.2",
    "nyc": "8.4.0",
    "rimraf": "2.5.4",
    "underscore-deep-extend": "1.1.5"
  }
}
