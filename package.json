{
  "name": "jcatalog-i18n",
  "version": "8.0.0",
  "description": "Provides i18n utils",
  "main": "index.js",
  "private": "true",
  "files": [
    "src",
    "index.js"
  ],
  "scripts": {
    "clean": "rimraf build esdoc",
    "lint": "eslint src",
    "jscs": "jscs src",
    "test": "mocha src/**/*.spec.js",
    "build": "npm run clean && npm run lint && npm run jscs && npm test && npm run build-umd && npm run build-umd-min",
    "build-umd": "cross-env NODE_ENV=development webpack --config webpack.config.js --progress --colors",
    "build-umd-min": "cross-env NODE_ENV=production webpack --config webpack.config.js --progress --colors",
    "esdoc": "esdoc -c esdoc.json"
  },
  "repository": {
    "type": "git",
    "url": "http://buildserver.jcatalog.com/git/js-i18n.git"
  },
  "author": {
    "name": "Dmitry Divin",
    "email": "dmitry.divin@jcatalog.com"
  },
  "license": "UNLICENSED",
  "readme": "README.md",
  "dependencies": {
    "moment": "^2.10.6",
    "moment-jdateformatparser": "git+https://github.com/MadMG/moment-jdateformatparser#88f3580f6f5a281fd678d760e146452bc2f04c97",
    "moment-timezone": "^0.4.0",
    "underscore": "^1.8.3",
    "underscore-deep-extend": "^0.0.5"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.0",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "chai": "^3.4.1",
    "cross-env": "^1.0.7",
    "css-loader": "^0.21.0",
    "esdoc": "^0.4.3",
    "esdoc-es7-plugin": "0.0.3",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^3.1.0",
    "jscs": "^2.9.0",
    "json-loader": "^0.5.4",
    "mocha": "^2.4.5",
    "rimraf": "^2.5.1",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.2"
  },
  "engines": {
    "node": ">=5.5.0",
    "npm": ">=3.6.0"
  }
}
